    <ImageView
        android:id="@+id/shapeTemp"
        android:layout_width="150dp"
        android:layout_height="150dp"

        android:background="@drawable/shape"
        app:layout_constraintBottom_toBottomOf="@+id/homeShape"

        app:layout_constraintEnd_toEndOf="@+id/homeShape"
        app:layout_constraintHorizontal_bias="0.095"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintStart_toStartOf="@+id/homeShape"
        app:layout_constraintTop_toTopOf="@+id/homeShape"
        app:layout_constraintVertical_bias="0.068" />

    <ImageView
        android:id="@+id/shapeHumid"
        android:layout_width="150dp"
        android:layout_height="150dp"

        android:layout_marginEnd="20dp"
        android:background="@drawable/shape"
        app:layout_constraintBottom_toBottomOf="@+id/shapeTemp"

        app:layout_constraintEnd_toEndOf="@+id/homeShape"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@+id/shapeTemp"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/shapeLight"
        android:layout_width="150dp"
        android:layout_height="150dp"

        android:layout_marginTop="20dp"

        android:background="@drawable/shape"
        app:layout_constraintStart_toStartOf="@+id/shapeTemp"
        app:layout_constraintTop_toBottomOf="@+id/shapeTemp"
        />


    <ImageView
        android:id="@+id/shapeMois"
        android:layout_width="150dp"
        android:layout_height="150dp"

        android:background="@drawable/shape"
        app:layout_constraintEnd_toEndOf="@+id/shapeHumid"
        app:layout_constraintTop_toTopOf="@+id/shapeLight" />


    <TextView
        android:id="@+id/titleHumid"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:fontFamily="@font/poppins"
        android:text="Humidity"
        android:drawableStart="@drawable/humidity"
        android:drawablePadding="5dp"
        app:layout_constraintEnd_toEndOf="@+id/shapeHumid"
        app:layout_constraintHorizontal_bias="0.516"
        app:layout_constraintStart_toStartOf="@+id/shapeHumid"
        app:layout_constraintTop_toTopOf="@+id/shapeHumid"
        />


    <TextView
        android:id="@+id/humid"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:drawablePadding="5dp"

        android:fontFamily="@font/poppins_medium"
        android:text="Waiting"
        android:textSize="25sp"
        app:layout_constraintEnd_toEndOf="@+id/shapeHumid"
        app:layout_constraintHorizontal_bias="0.516"
        app:layout_constraintStart_toStartOf="@+id/shapeHumid"
        app:layout_constraintTop_toBottomOf="@+id/titleHumid" />


    <TextView
        android:id="@+id/syncHumid"
